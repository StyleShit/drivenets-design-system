{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"enableGlobDot": true,
	"useGitignore": true,
	"dictionaries": ["typescript", "npm", "filetypes", "softwareTerms"],
	"words": [
		"attw",
		"autoboot",
		"autodocs",
		"borderless",
		"clickability",
		"controlify",
		"drivenets",
		"evenodd",
		"findlast",
		"fromentries",
		"gnmi",
		"Linsley",
		"matchall",
		"netgen",
		"nolyfill",
		"onwarn",
		"overscan",
		"Oxfmt",
		"reorderable",
		"retryable",
		"subcomponents",
		"syncpack",
		"tosorted",
		"Turborepo",
		"unhover"
	],
	"ignorePaths": [
		".cursor",
		".git",
		".github",
		".idea",
		".vscode",
		".cspell",
		".turbo",
		".DS_Store",
		"**/dist/**",
		"**/coverage/**",
		"**/storybook-static/**",
		"**/node_modules/**",
		"pnpm-lock.yaml"
	]
}
